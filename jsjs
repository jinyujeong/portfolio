                                    break;
                                case 43:
                                    this.$ = i.preparePath(!1, o[a], this._$);
                                    break;
                                case 44:
                                    o[a - 2].push({
                                        part: i.id(o[a]),
                                        original: o[a],
                                        separator: o[a - 1]
                                    }), this.$ = o[a - 2];
                                    break;
                                case 45:
                                    this.$ = [{
                                        part: i.id(o[a]),
                                        original: o[a]
                                    }];
                                    break;
                                case 46:
                                    this.$ = [];
                                    break;
                                case 47:
