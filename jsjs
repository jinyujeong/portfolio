                                    };
                                    break;
                                case 36:
                                    this.$ = {
                                        type: "NumberLiteral",
                                        value: Number(o[a]),
                                        original: Number(o[a]),
                                        loc: i.locInfo(this._$)
                                    };
                                    break;
                                case 37:
                                    this.$ = {
                                        type: "BooleanLiteral",
                                        value: "true" === o[a],
                                        original: "true" === o[a],
                                        loc: i.locInfo(this._$)
                                    };
                                    break;
                                case 38:
