                        return t.replace(/(rgba?\(\s*)?(\b[a-z]+\b)/g, function(t, e, n) {
                            return C.Lists.colorNames.hasOwnProperty(n) ? (e || "rgba(") + C.Lists.colorNames[n] + (e ? "" : ",1)") : e + n
                        })
                    },
