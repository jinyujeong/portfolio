            }).end());
            for (var r = e.currentSlide, o = r + e.options.slidesToShow; r < o; r++) e.options.focusOnChange ? e.$slides.eq(r).attr({
                tabindex: "0"
            }) : e.$slides.eq(r).removeAttr("tabindex");
            e.activateADA()
        }, e.prototype.initArrowEvents = function() {
